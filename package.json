		"node-cache": "^5.1.0",
		"passport-ldapauth": "^2.1.3",
		"radius": "~1.1.4",
		"ts-node": "^8.6.2",
		"type-cacheable": "^4.0.0",
		"yargs": "~15.1.0"
	},
	"license": "GPLv3",
	"devDependencies": {
		"@types/ldapjs": "^1.0.5",
		"@types/radius": "0.0.28",
		"@hokify/eslint-config": "^0.2.4",
		"eslint": "^6.8.0",
		"prettier": "^1.19.1",
		"typescript": "^3.8.2"
	}
